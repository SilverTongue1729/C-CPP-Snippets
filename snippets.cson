'.source.cpp':
  'for_loop':
    'prefix': 'f_'
    'body': 'for_(${1:i},${2:0},${3:n}){\n\t$0\n}'

  'for loop':
    'prefix': 'for'
    'body': 'for (${1:i}=${2:0};${1:i}<${3:n};${1:i}++){\n\t$0\n}'

  'while':
    'prefix': 'while'
    'body': 'while ($1){\n\t$0\n}'

  'do while':
    'prefix': 'do'
    'body': 'do {\n\t$1\n} while();'

  'cin':
    'prefix': 'cin'
    'body': 'cin >> $1;'

  'cout':
    'prefix': 'cout'
    'body': 'cout << $1 ${2:<< ${3:\'$0${4:\\\\n}\'}};'

  '#define':
    'prefix': 'def'
    'body': '#define $1'

  '#include <>':
    'prefix': 'Inc'
    'body': '#include <${2:${1:bits/stdc++}.h}>'

  'if':
    'prefix': 'if'
    'body': 'if ($1){\n\t$0\n}'

  'else':
    'prefix': 'el'
    'body': 'else{\n\t$1\n}'

  'if else':
    'prefix': 'ife'
    'body': 'if ($1){\n\t$0\n}\nelse{\n\t\n}'

  'if elseif':
    'prefix': 'iff'
    'body': 'if ($1){\n\t$0\n}\nelse if(){\n\t\n}'

  'if elseif else':
    'prefix': 'iffe'
    'body': 'if ($1){\n\t$0\n}\nelse if(){\n\t\n}\nelse{\n\t\n}'

  'else if ':
    'prefix': 'elif'
    'body': 'else if ($1){\n\t$0\n}'

  'switch':
    'prefix': 'switch'
    'body': 'switch ($1) {\n\tcase :\n\t$0\tbreak;\n}'

  'case':
    'prefix': 'case'
    'body': 'case $1: \n\t$0\n\tbreak;'

  'return':
    'prefix': 'return'
    'body': 'return ${1:0};'

  'main()':
    'prefix': 'main'
    'body': 'int main(){\n\t$1\n\treturn 0;\n}'

  'printf':
    'prefix': 'printf'
    'body': 'printf("%${1:d}\\\\n", $2);'

  'scanf':
    'prefix': 'scanf'
    'body': 'scanf("%${1:d}", &$2);'

  'fprintf':
    'prefix': 'fprintf'
    'body': 'fprintf($1,"%${2:d}\\\\n",$3);'

  'fopen':
    'prefix': 'fopen'
    'body': 'fopen("$1.txt", "${2:w}");\n$0\n\nfclose();'

  'fclose':
    'prefix': 'fclose'
    'body': 'fclose($1);'

  'int':
    'prefix': 'int'
    'body': 'int $1;'

  'long long':
    'prefix': 'll'
    'body': 'll $1;'

  'vector':
    'prefix': 'vector'
    'body': 'vector<${1:int}> ${2:v}${3:(${4:LIM1},${5:0})};'

  'set':
    'prefix': 'set'
    'body': 'set<${1:int}> $2;'

  'multiset':
    'prefix': 'multiset'
    'body': 'multiset<${1:int}> $2;'

  'insert':
    'prefix': 'insert'
    'body': 'insert($1);'

  'erase':
    'prefix': 'erase'
    'body': 'erase($1);'

  'string':
    'prefix': 'str'
    'body': 'string $1;'

  'struct':
    'prefix': 'st'
    'body': 'struct $1{\n\t$0\n};'

  'void':
    'prefix': 'void'
    'body': 'void $1(){\n\t$0\n}'

  'typedef':
    'prefix': 'td'
    'body': 'typedef ${1:int} $2;'

  'using namespace':
    'prefix': 'using'
    'body': 'using namespace ${1:std};'

  'namespace':
    'prefix': 'ns'
    'body': 'namespace $1{\n}'

  'template <typename>':
    'prefix': 'tp'
    'body': 'template <${1:typename} $2>'

