  'fastscan':
    'prefix': 'fs'
    'body': '''
int fastscan(int &number){
  bool negative = 0;
  register int c;

  number = 0;
  c = getchar();
  if (c=='-'){
    negative = 1;
    c = getchar();
  }

  for (; (c>='0' && c<='9'); c=getchar())
    number = number*10+c-'0';
  if (negative)
    number *= -1;
  return number;
}
'''

  'fastprint':
    'prefix': 'fp'
    'body': '''
 void fastprint(int num){
    bool neg = 0;
    if (num < 0)
      num *= -1, neg = 1;
    if (neg)
      putchar('-');

    char snum[65];
    int i = 0;
    do{
        snum[i++] = num % 10 + '0';
        num /= 10;
    } while (num);

    --i;

    while (i >= 0)
      putchar(snum[i--]);
}
'''
